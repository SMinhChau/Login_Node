<%for(var i=0; i<users.length;i++) {%>

<tr class="text-center">
    <td class="align-center"><%= i+1 %></td>
    <td><%= users[i].name %></td>
    <td><%= users[i].email %></td>
    <td><%= users[i].gender %></td>
    <td><%= users[i].status %></td>
    <td>
        <a href="/update-user" class="btn border-shawdow update"><i class="fa fa-pencil" aria-hidden="true"></i></a>
        <a href="#" class="btn border-shawdow delete"><i class="fa fa-trash" aria-hidden="true"></i></a>
    </td>
 </tr>

 <%}%>